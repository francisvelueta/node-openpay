<%- layout('layouts/main.ejs') %>
<form class="d-flex justify-content-center mt-5" action="/signup" method="POST">
  <fieldset class="col-lg-5 border border-dark d-flex justify-content-center">
     <legend class="d-flex justify-content-center">Inicio de Sesion</legend>
    <div class="d-flex justify-content-center form-group">
    <input type="email" class="form-control"  name= "email" aria-describedby="emailHelp" placeholder="Email">
    </div>
    <div class="d-flex justify-content-center form-group">
    <input type="password" class="form-control" name= "password" placeholder="Password">
    </div>
      <button type="submit" class="d-flex justify-content-center mb-2 ml-1 btn btn-primary" >Entrar</button>
     <fieldset>
</form>
